    }

    private int vazia (int capacidade){
        if (primeiro ==0) {
            System.out.print("A pilha está vazia");
        }
        return capacidade;
    }
    private int cheia(int capacidade){
        if (primeiro == capacidade){
            System.out.print("a pilha está cheia");
        }
        return capacidade;
    }
    public  void main(String[] args) {
        inserir(10);
        remover(10);
        imprimir(10);
        vazia(10);
        cheia(15);
        

    }
}
