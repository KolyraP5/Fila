public class Fila {
    private int primeiro;
    private int ultimo;
    private int dados[];

    public Fila  (int dados[]){
        ultimo = -1;
        primeiro = 0;
    }
    private int inserir (int dados){
        primeiro ++;
        dados [primeiro] = primeiro;
        return primeiro;
    }

    private int remover (int ultimo){
        primeiro --;
        dados [primeiro] = primeiro;
        return primeiro;
    }
    private void imprimir (int dados){
